<div class="gallery" id="gallery">
  {{#each fuzzles}}
    <div class="gallery-item">
      <div class="content">
        <img src="/assets/images/{{file}}" title="{{title}}" />
        <div class="tags">
        {{# each tags}}
          <a href="/tag/{{this}}">{{this}},</a>
        {{/each}}
        </div>
      </div>
    </div>
  {{/each}}
</div>

